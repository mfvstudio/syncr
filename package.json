    "load": "./loadVars.sh"
  },
  "engines": {
    "node": ">=20.9"
  },
  "dependencies": {
    "@cdktf/provider-google": "^16.2.0",
    "cdktf": "^0.21.0",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
