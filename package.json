{
  "name": "SyncRCDKTF",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "begin": "npm run get && npm install",
    "build": "tsc",
    "synth": "cdktf synth ./src",
    "compile": "tsc --pretty",
    "watch": "tsc -w",
    "test": "jest",
    "clean": "rm -rf .gen cdktf.out node_modules dist terraform.*tfstate*",
    "test:watch": "jest --watch",
    "upgrade": "npm i cdktf@latest cdktf-cli@latest",
    "upgrade:next": "npm i cdktf@next cdktf-cli@next",
    "load": "./loadVars.sh",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint ./src/*/** --fix",
    "deploy": "cdktf deploy Syncr-ComputeStack "
  },
  "engines": {
    "node": ">=20.9"
  },
  "dependencies": {
    "@cdktf/provider-google": "^16.2.0",
    "cdktf": "^0.21.0",
    "constructs": "^10.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.3",
    "eslint": "^9.29.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1"
  }
}
